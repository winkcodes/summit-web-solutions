{
  "name": "summit-web-solutions",
  "type": "module",
  "version": "1.0.0",
  "description": "Summit Web Solutions - Professional Web Development & Digital Marketing Services",
  "keywords": ["web development", "digital marketing", "SEO", "responsive design", "Astro", "React", "TypeScript"],
  "author": "Summit Web Solutions",
  "license": "MIT",
  "homepage": "https://summitweb.solutions",
  "repository": {
    "type": "git",
    "url": "https://github.com/summit-web-solutions/website.git"
  },
  "scripts": {
    "dev": "astro dev --host",
    "dev:fresh": "npm run clean:cache && astro dev --host",
    "start": "astro dev",
    "build": "astro check && astro build",
    "preview": "astro preview",
    "astro": "astro",
    "check": "astro check",
    "format": "prettier --write .",
    "lint": "eslint . --ext .astro,.js,.jsx,.ts,.tsx",
    "clean": "rm -rf dist .astro",
    "clean:cache": "rm -rf .astro dist node_modules/.vite",
    "typecheck": "tsc --noEmit"
  },
  "dependencies": {
    "@astrojs/react": "^3.6.2",
    "@astrojs/sitemap": "^3.1.6",
    "@astrojs/tailwind": "^5.1.1",
    "@fontsource/inter": "^5.0.0",
    "@types/react": "^18.3.12",
    "@types/react-dom": "^18.3.1",
    "astro": "^4.15.11",
    "lucide-react": "^0.468.0",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "tailwindcss": "^3.4.14"
  },
  "devDependencies": {
    "@tailwindcss/typography": "^0.5.15",
    "prettier": "^3.3.3",
    "prettier-plugin-astro": "^0.14.1",
    "typescript": "^5.6.3"
  },
  "engines": {
    "node": ">=18.0.0"
  }
